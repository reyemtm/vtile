{
  "name": "vtile",
  "version": "1.0.2",
  "description": "A simple Node and command line tool to create vector tiles (directory or mbtiles) in JavaScript from one or more geojson files using geojson2mvt, geojson-vt, and TurfJS.",
  "main": "index.js",
  "bin": {
    "vtile": "./bin/vtile.js"
  },
  "scripts": {
    "start": "node bin/vtile -i data/ohio.geojson -p",
    "build": "node __tests__/test.js",
    "test": "jest index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerospatial/vtile.git"
  },
  "keywords": [
    "vector tiles",
    "vector-tiles",
    "geojson",
    "turfjs",
    "spatial",
    "mbtiles",
    "maps",
    "gis"
  ],
  "author": "Malcolm Meyer | zeroSpatial",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zerospatial/vtile/issues"
  },
  "homepage": "https://github.com/zerospatial/vtile#readme",
  "dependencies": {
    "@mapbox/geojson-rewind": "^0.5.1",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "better-sqlite3": "^7.5.0",
    "cliclopts": "^1.1.1",
    "file-extension": "^4.0.5",
    "fs-extra": "^10.0.0",
    "geojson-validation": "^1.0.2",
    "geojson-vt": "^3.2.1",
    "minimist": "^1.2.5",
    "opener": "^1.5.2",
    "static-server": "^3.0.0",
    "uuid": "^8.3.2",
    "vt-pbf": "^3.1.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "jest": "^27.5.1"
  }
}
